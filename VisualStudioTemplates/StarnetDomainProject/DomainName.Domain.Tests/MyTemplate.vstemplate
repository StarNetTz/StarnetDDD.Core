<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>DomainName.Domain.Tests</Name>
    <Description>BDD Tests for domain</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>DomainName.Domain.Tests</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>__TemplateIcon.ico</Icon>
    <CreateInPlace>true</CreateInPlace>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="Accounting.Domain.Tests.csproj" File="Accounting.Domain.Tests.csproj" ReplaceParameters="true">
      <Folder Name="CompanyTests" TargetFolderName="CompanyTests">
        <ProjectItem ReplaceParameters="true" TargetFileName="_ServiceSpec.cs">_ServiceSpec.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="RegisterCompanyTests.cs">RegisterCompanyTests.cs</ProjectItem>
      </Folder>
    </Project>
  </TemplateContent>
</VSTemplate>